{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "Promotion": false,
    "Promotion.Discounts": false,
    "Suggestion.User": {
      "EnabledFor": [
        {
          "Name": "MusicStore.Browser",
          "Parameters": {
            "AllowedBrowsers": [
              "Chrome",
              "Firefox"
            ]
          }
        }
      ]
    }
  }
}
